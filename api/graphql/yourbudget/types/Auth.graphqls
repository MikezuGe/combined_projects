type Auth {
  userId: ID
  sessionId: ID
  sessionTimeout: String
  sessionStarted: String
  user: User
}

type AuthUpdate {
  ok: Int
  nModified: Int
  n: Int
}

type AuthDelete {
  ok: Int
  n: Int
}

input AuthInput {
  email: String!
  password: String!
}

type Query {
  auth(_id: ID!): Auth
  auths: [Auth]
}

type Mutation {
  createAuth(input: AuthInput!): Auth
  updateAuth(sessionId: ID): AuthUpdate
  deleteAuth(_id: ID!): AuthDelete
}
