{
  "name": "combined_projects",
  "version": "1.0.0",
  "description": "This is a repository of multiple webpages.\r Each webpage has its own folder in the root folder.\r Each webpage's folder has an index file that will be served when a folder is accessed",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development run-p start:*",
    "start:webpack": "npx webpack --watch",
    "start:nodemon": "npx nodemon --watch index.js --watch api --watch utility index.js",
    "deploy": "run-s deploy:build deploy:add \"deploy:commit -- {1}\" --",
    "deploy:build": "cross-env NODE_ENV=production npx webpack",
    "deploy:add": "git add .",
    "deploy:commit": "git commit -a -m ",
    "postdeploy": "git push -f"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "mongoose": "^5.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.8.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3"
  }
}
