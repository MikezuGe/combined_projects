{
  "name": "combined_projects",
  "version": "1.0.0",
  "description": "This is a repository of multiple webpages.\r Each webpage has its own folder in the root folder.\r Each webpage's folder has an index file that will be served when a folder is accessed",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development run-p start:*",
    "start:webpack": "webpack --watch",
    "start:nodemon": "nodemon --watch index.js --watch api --watch utility index.js",
    "deploy": "run-s deploy:build deploy:add \"deploy:commit -- {1}\" --",
    "deploy:build": "cross-env NODE_ENV=production webpack",
    "deploy:add": "git add .",
    "deploy:commit": "git commit -a -m ",
    "postdeploy": "git push -f"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "mongoose": "^5.2.13",
    "path-to-regexp": "^2.4.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1",
    "socket.io": "^2.1.1",
    "styled-components": "^3.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  }
}
